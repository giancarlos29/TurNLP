{
	"name": "youtubeChannelBase",
	"properties": {
		"activities": [
			{
				"name": "GetChannelBase",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010"
					},
					"sink": {
						"type": "AzureSqlSink"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "YoutubeDataset",
						"type": "DatasetReference",
						"parameters": {
							"RelativeURI": {
								"value": "@{variables('Resource')}@{variables('part')}&key=@{variables('Key')}&forUsername=@{variables('username')}&fields=@{variables('filtro')}",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlTable",
						"type": "DatasetReference",
						"parameters": {
							"TableName": "Youtube.Channel"
						}
					}
				]
			}
		],
		"parameters": {
			"Table": {
				"type": "string",
				"defaultValue": "youtube.channel"
			}
		},
		"variables": {
			"Key": {
				"type": "String",
				"defaultValue": "AIzaSyAHfANp9kumRlADnwOjrL0-tglGvS7U9gw"
			},
			"part": {
				"type": "String",
				"defaultValue": "?part=id, snippet"
			},
			"Resource": {
				"type": "String",
				"defaultValue": "channels"
			},
			"filtro": {
				"type": "String",
				"defaultValue": "items(id,snippet(title,description,publishedAt))"
			},
			"username": {
				"type": "String",
				"defaultValue": "clippers1970"
			},
			"filtroStatistics": {
				"type": "String",
				"defaultValue": "items(id,statistics(viewCount,subscriberCount,videoCount))"
			},
			"columnsChannelBase": {
				"type": "String",
				"defaultValue": "channelid,published,Description,Title,upgrateDate"
			}
		},
		"folder": {
			"name": "SocialMedia/Youtube"
		},
		"annotations": []
	}
}